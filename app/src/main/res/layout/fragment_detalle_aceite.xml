<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fernandez.pablo.la24gnc.View.DetalleTurno.DetalleAceiteFragment">


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">


        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="332dp"
            android:layout_height="33dp"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="140dp"
            android:orientation="horizontal"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_weight="1"
                android:text="VALOR INICIAL"
                android:textAlignment="textStart"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_weight="1"
                android:text="VALOR FINAL"
                android:textAlignment="textStart"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_weight="1"
                android:text="DIFERENCIA"
                android:textAlignment="textStart"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="332dp"
            android:layout_height="37dp"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"
            app:layout_constraintHorizontal_bias="0.538">

            <TextView
                android:id="@+id/aforadorAceite1VI"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:text="1111"
                android:textAlignment="textStart"
                android:textSize="18dp" />

            <TextView
                android:id="@+id/aforadorAceite1VF"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_weight="1"
                android:text="2222"
                android:textAlignment="textStart"
                android:textSize="18dp" />

            <TextView
                android:id="@+id/difAceite"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:text="1111"
                android:textAlignment="textStart"
                android:textSize="18dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="195dp"
            android:layout_height="26dp"
            android:orientation="horizontal"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="24dp"
            android:id="@+id/linearLayout2">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.03"
                android:text="TOTAL ($)"
                android:textAlignment="textEnd"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_conversion_absoluteHeight="21dp"
                tools:layout_conversion_absoluteWidth="220dp"
                tools:layout_conversion_absoluteX="15dp"
                tools:layout_conversion_absoluteY="250dp"
                tools:layout_editor_absoluteX="64dp"
                tools:layout_editor_absoluteY="58dp" />

            <TextView
                android:id="@+id/totalDineroAceite"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="$ 1000"
                android:textAlignment="textEnd"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_conversion_absoluteHeight="21dp"
                tools:layout_conversion_absoluteWidth="110dp"
                tools:layout_conversion_absoluteX="235dp"
                tools:layout_conversion_absoluteY="250dp"
                tools:layout_editor_absoluteX="235dp"
                tools:layout_editor_absoluteY="250dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="194dp"
            android:layout_height="29dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="9dp"
            android:orientation="horizontal"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout2">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="TOTAL (lts)"
                android:textAlignment="textEnd"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_conversion_absoluteHeight="21dp"
                tools:layout_conversion_absoluteWidth="220dp"
                tools:layout_conversion_absoluteX="15dp"
                tools:layout_conversion_absoluteY="218dp"
                tools:layout_editor_absoluteX="15dp"
                tools:layout_editor_absoluteY="218dp" />

            <TextView
                android:id="@+id/totalLts"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="44444 m3"
                android:textAlignment="textEnd"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_conversion_absoluteHeight="21dp"
                tools:layout_conversion_absoluteWidth="110dp"
                tools:layout_conversion_absoluteX="235dp"
                tools:layout_conversion_absoluteY="218dp"
                tools:layout_editor_absoluteX="235dp"
                tools:layout_editor_absoluteY="218dp" />
        </LinearLayout>


    </android.support.constraint.ConstraintLayout>

</FrameLayout>
